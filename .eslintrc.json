{
    "extends" : "airbnb",
    "plugins" : [
      "react",
      "jsx-a11y",
      "import"
    ],
    "parser": "babel-eslint",
    "rules": {
      "react/jsx-filename-extension" : 0,
      "react/prefer-stateless-function" : 0,
      "no-use-before-define": 0,
      "global-require": 0,
      "class-methods-use-this": 0,
      "spaced-comment": 0,
      "no-nested-ternary": 0,
      "object-curly-newline": 0,
      "react/jsx-no-bind": 0,
      "quotes": 0,
      "prefer-destructuring": 0,
      "react/destructuring-assignment": 0
    }
}